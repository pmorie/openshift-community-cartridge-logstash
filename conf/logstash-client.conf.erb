input {
unix { path => "<%= ENV['OPENSHIFT_LOGSTASH_SOCKET']%>"
       type => "metrics" }
file { path => "<%= ENV['OPENSHIFT_HOMEDIR']%>**/logs/*"
       type => "logs"
     }
}
output {
  redis { host => "<%= ENV['OPENSHIFT_REDIS_HOST']%>"
        port => "<%= ENV['OPENSHIFT_REDIS_PORT']%>"
        password => "<%= ENV['OPENSHIFT_REDIS_PASSWORD']%>"
        data_type => "list"
        key => "osedata" }
}
